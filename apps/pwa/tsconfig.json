{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "jsx": "react-jsx",
    "allowJs": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,

    "paths": {
      "@/*": [
        "./src/*",
        "../web/app/*"
      ],
      "next/link": [
        "./src/components/next-link.tsx"
      ],
      "next/*": [
        "./src/components/next-link.tsx"
      ],
    }
  },
}
